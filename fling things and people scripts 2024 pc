--♥func♥

local function chat_print(msg)
    privateProperties.Text = "♥SIS♥ "..msg
    StarterGui:SetCore("ChatMakeSystemMessage",privateProperties)
end

local function delete_clone_grab()
    while a ~= 0 do
        w:FindFirstChild(a):Destroy()
        a=a-1
    end
end

local function clone_grab(model)
    a=a+1
    model:Clone().Parent = w
    model:Destroy()
    w.GrabParts.BeamPart:Destroy()
    w.GrabParts.Name = a
end

local function tp_damage(model)
    if onf then
        while onf == true do
            m.Hole.PoisonBigHole.PoisonHurtPart.Position = model.GrabPart.Position
            m.Hole.PoisonSmallHole.PoisonHurtPart.Position = model.GrabPart.Position
            m.FactoryIsland.PoisonContainer.PoisonHurtPart.Position = model.GrabPart.Position
            m.FactoryIsland.fff.PoisonHurtPart.Position = model.GrabPart.Position
            m.AlwaysHereTweenedObjects.InnerUFO.Object.ObjectModel.PaintPlayerPart.Position = model.GrabPart.Position
            if w.sfdweasfSpawnedInToys:FindFirstChild("Campfire") ~= nil then
                w.sfdweasfSpawnedInToys.Campfire.FirePlayerPart.Position =  model.GrabPart.Position
            end
            Wait(0.00000000000000000000000000000000000000000001)
        end
    else
        m.Hole.PoisonBigHole.PoisonHurtPart.Position = Vector3.new(0,-20,0)
        m.Hole.PoisonSmallHole.PoisonHurtPart.Position = Vector3.new(0,-20,0)
        m.FactoryIsland.PoisonContainer.PoisonHurtPart.Position = Vector3.new(0,-20,0)
        m.FactoryIsland.fff.PoisonHurtPart.Position = Vector3.new(0,-20,0)
        m.AlwaysHereTweenedObjects.InnerUFO.Object.ObjectModel.PaintPlayerPart.Position = Vector3.new(0,-20,0)
        if w.sfdweasfSpawnedInToys:FindFirstChild("Campfire") ~= nil then
            w.sfdweasfSpawnedInToys.Campfire.FirePlayerPart.Position = Vector3.new(0,-20,0)
        end
        
    end
end

local function kill (chto)
    if killg then
        chto.Parent:FindFirstChild("Humanoid"):TakeDamage(100)
    else
        chto.Parent:FindFirstChild("Humanoid").Health = 100
    end
end

local function clip (chto)
    if clippp then
        chto.Parent:FindFirstChild("HumanoidRootPart").CanCollide = false
        chto.Parent:FindFirstChild("Left Arm").CanCollide = false
        chto.Parent:FindFirstChild("Left Leg").CanCollide = false
        chto.Parent:FindFirstChild("Right Arm").CanCollide = false
        chto.Parent:FindFirstChild("Right Leg").CanCollide = false
        chto.Parent.Torso.CanCollide = false
        chto.Parent.Head.CanCollide = false
    else
        chto.Parent:FindFirstChild("HumanoidRootPart").CanCollide = true
        chto.Parent:FindFirstChild("Left Arm").CanCollide = true
        chto.Parent:FindFirstChild("Left Leg").CanCollide = true
        chto.Parent:FindFirstChild("Right Arm").CanCollide = true
        chto.Parent:FindFirstChild("Right Leg").CanCollide = true
        chto.Parent.Torso.CanCollide = true
        chto.Parent.Head.CanCollide = true
    end
end

local function tp_part(ito,da,wat)
    if da == 1 then
        ito.Position = ito.Position + Vector3.new(30,0,0)
        wait(wat)
        ito.Position = ito.Position + Vector3.new(0,30,0)
        wait(wat)
        ito.Position = ito.Position + Vector3.new(0,0,30)
        wait(wat)
    elseif da == 0 then
        ito.Position = ito.Position + Vector3.new(-60,0,0)
        wait(wat)
        ito.Position = ito.Position + Vector3.new(0,-60,0)
        wait(wat)
        ito.Position = ito.Position + Vector3.new(0,0,-60)
        wait(wat)
        ito.Position = ito.Position + Vector3.new(30,0,0)
        wait(wat)
        ito.Position = ito.Position + Vector3.new(0,30,0)
        wait(wat)
        ito.Position = ito.Position + Vector3.new(0,0,30)
    end
end

local function kick1 (model)
    if kickkk then
        while kickkk == true do
            tp_part(model.GrabPart,1,0.1)
            tp_part(model.GrabPart,0,0.1)
            wait()
        end
    end
end

local function kick2 (model)
    if kickkk then
        while kickkk == true do
            tp_part(model.DragPart.DragAttach,1,0)
            wait()
            tp_part(model.DragPart.DragAttach,0,0)
        end
    end
end

local function kick3 (model)
    local hz = model["GrabPart"]["WeldConstraint"].Part1
    if kickkk and hz ~= nil then
        while kickkk == true do
            model.DragPart.DragAttach.Position = model.DragPart.DragAttach.Position + Vector3.new(0,10,0)
            wait()
        end
    elseif kickkk and hz == nil then
        while kickkk == true do
            model.DragPart.DragAttach.Position = model.DragPart.DragAttach.Position + Vector3.new(0,100,0)
            wait()
        end
    end
end

local function check_all (model)
    if model ~= nil and model.Name == "GrabParts" then
        last_model = model
        task.spawn(tp_damage,model)
        if kickkk2 == 0 then
            task.spawn(kick1,model)
        elseif kickkk2 == 1 then
            task.spawn(kick2,model)
        elseif kickkk2 == 2 then
            task.spawn(kick3,model)
        end
        local chto = model["GrabPart"]["WeldConstraint"].Part1
        local chto2 = model["GrabPart"]["WeldConstraint"].Part1
        last_chto2 = chto2
        if chto.Name == "Head" or chto.Name == "Torso" or chto.Name == "Right Arm" or chto.Name == "Right Leg" or chto.Name == "Left Arm" or chto.Name == "Left Leg" or chto.Name == "HumanoidRootPart" then
            last_chto = chto
            task.spawn(clip,chto)
            task.spawn(kill,chto)
        end
    else
        task.spawn(clip,last_chto)
        task.spawn(kill,last_chto)
        task.spawn(tp_damage,last_model)
        if kickkk2 == 0 then
            task.spawn(kick1,last_model)
        elseif kickkk2 == 1 then
            task.spawn(kick2,last_model)
        elseif kickkk2 == 2 then
            task.spawn(kick3,last_model)
        end
    end
end

local function anti_lag ()
    if antilaggg then
        w.PlotItems.Plot1.Parent = RS
        w.PlotItems.Plot2.Parent = RS
        w.PlotItems.Plot3.Parent = RS
        w.PlotItems.Plot4.Parent = RS
        w.PlotItems.Plot5.Parent = RS
    else
        RS.Plot1.Parent = w.PlotItems
        RS.Plot2.Parent = w.PlotItems
        RS.Plot3.Parent = w.PlotItems
        RS.Plot4.Parent = w.PlotItems
        RS.Plot5.Parent = w.PlotItems
    end
end

